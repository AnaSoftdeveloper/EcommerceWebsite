@model List<CustomerDto>


<h1>Customer List</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Customer Name
            </th>
            <th>
               Customer Email
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model.OrderBy(u => u.customerName))
        {
            <tr>
                <td>@obj.customerName</td>
                <td>@obj.customerEmail</td>
            </tr>
        }

    </tbody>
</table>